<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.nam.dormitorymanage.backend.dao.StudentInfoDao">

    <select id="getStudents" resultType="StudentInfo">
        select num,name,school,major,teacher_name,teacher_tel,building_num,room_num
        from student_info
        where building_num = #{buildingNum}
    </select>

</mapper>